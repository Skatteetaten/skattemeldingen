{
  "name": "aurora",
  "version": "1.0.0",
  "description": "Documentation for The Aurora Platform",
  "license": "Apache-2.0",
  "scripts": {
    "prestart": "npm run clean",
    "start": "gatsby develop --verbose",
    "prebuild": "npm run clean",
    "build": "gatsby build --prefix-paths",
    "deploy": "gh-pages -d public -p 0",
    "prettier:fix": "prettier --write '{src,docs}/**/*.{js,jsx,css,md}'",
    "test": "echo \"Error: no test specified\"",
    "clean": "rimraf public .cache"
  },
  "devDependencies": {
    "gh-pages": "2.2.0",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@skatteetaten/frontend-components": "3.2.0",
    "classnames": "2.2.6",
    "gatsby": "2.24.52",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-remark-autolink-headers": "2.3.11",
    "gatsby-remark-copy-linked-files": "2.3.12",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-remark-prismjs": "3.5.11",
    "gatsby-source-filesystem": "2.3.27",
    "gatsby-transformer-remark": "2.8.31",
    "github-slugger": "1.3.0",
    "lodash": "4.17.15",
    "prismjs": "1.21.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "rehype-react": "5.0.1",
    "slash": "3.0.0"
  }
}
